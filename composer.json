{
    "name": "votintsev/laravel-test-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Votintsev",
            "email": "nikolay@votintsev.ru"
        }
    ],
    "require": {
        "illuminate/support": ">=5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Votintsev\\TestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Votintsev\\TestApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Votintsev\\TestApi\\TestApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
