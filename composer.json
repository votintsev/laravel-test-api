{
    "name": "votintsev/laravel-public-seeding",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Votintsev",
            "email": "nikolay@votintsev.ru"
        }
    ],
    "require": {
        "illuminate/support": "^7.0",
        "illuminate/http": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Votintsev\\PublicSeeding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Votintsev\\PublicSeeding\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Votintsev\\PublicSeeding\\PublicSeedingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
